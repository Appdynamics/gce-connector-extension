<?xml version="1.0" encoding="UTF-8"?>
<image-repository-types>

    <image-repository-type>
        <name>Google Compute Engine</name>
        <description>Google Compute Engine</description>
        <connector-impl-class-name>
            com.singularity.ee.connectors.gce.GCEConnector
        </connector-impl-class-name>
        <property-definitions>
            <property-definition>
                <name>Service Account ID</name>
                <description>Service Account ID</description>
                <required>true</required>
                <type>STRING</type>
                <default-string-value></default-string-value>
                <string-max-length>90</string-max-length>
                <allowed-string-values></allowed-string-values>
                <default-file-value></default-file-value>
            </property-definition>
            <property-definition>
                <name>Service Account P12 File Path</name>
                <description>Service Account P12 File Path</description>
                <required>true</required>
                <type>STRING</type>
                <default-string-value></default-string-value>
                <string-max-length>150</string-max-length>
                <allowed-string-values></allowed-string-values>
                <default-file-value></default-file-value>
            </property-definition>
        </property-definitions>
    </image-repository-type>
</image-repository-types>