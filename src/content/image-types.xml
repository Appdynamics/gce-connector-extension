<?xml version="1.0" encoding="UTF-8"?>
<image-types>
	<image-type>
        <name>Google Compute Engine Image</name>
        <description>Google Compute Engine Image</description>
		<property-definitions>
            <property-definition>
                <name>Machine Type</name>
                <description>Machine Type</description>
                <required>true</required>
                <type>STRING</type>
                <default-string-value></default-string-value>
                <string-max-length>80</string-max-length>
                <allowed-string-values>f1-micro,g1-small,n1-highcpu-2,n1-highcpu-4,n1-highcpu-8,n1-highmem-2,n1-highmem-4,n1-highmem-8,n1-standard-1,n1-standard-2,n1-standard-4,n1-standard-8</allowed-string-values>
                <default-file-value></default-file-value>
            </property-definition>
            <property-definition>
                <name>Image</name>
                <description>Image</description>
                <required>true</required>
                <type>STRING</type>
                <default-string-value></default-string-value>
                <string-max-length>80</string-max-length>
                <allowed-string-values>centos-6-v20131120,backports-debian-7-wheezy-v20131127,debian-7-wheezy-v20131120</allowed-string-values>
                <default-file-value></default-file-value>
            </property-definition>
		</property-definitions>
		<supported-compute-center-types>
			<compute-center-type>Google Compute Engine</compute-center-type>
		</supported-compute-center-types>
	</image-type>
</image-types>